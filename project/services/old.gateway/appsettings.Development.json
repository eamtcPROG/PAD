{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "user": {
        "ClusterId": "user",
        "Match": {
          "Path": "/user/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      },
      "ticketorder": {
        "ClusterId": "ticketorder",
        "Match": {
          "Path": "/ticketorder/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "user": {
        "Destinations": {
          "userApi": {
            "Address": "http://user:4001"
          }
        }
      },
      "ticketorder": {
        "Destinations": {
          "ticketOrderApi": {
            "Address": "http://ticketorder:4000"
          }
        }
      }
    }
  }
}